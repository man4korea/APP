# 전자계산기 프로젝트 데이터베이스 설정
# MySQL/MariaDB 연결 설정
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_calculator_mysql_password
MYSQL_DATABASE=calculator_app

# MySQL MCP 보안 설정
ALLOW_INSERT_OPERATION=true
ALLOW_UPDATE_OPERATION=true
ALLOW_DELETE_OPERATION=false
MYSQL_POOL_SIZE=5
MYSQL_QUERY_TIMEOUT=15000

# 프로젝트별 추가 설정
PROJECT_NAME=Calculator_WebApp
ENV_TYPE=development